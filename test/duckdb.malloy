sql: jp_daily is ||
  select *
  from 'https://raw.githubusercontent.com/kj-9/covid19jp/master/data-raw/dist/jp_daily.csv'
  limit 10
;;

sql: pref_daily is ||
  select * from 'https://raw.githubusercontent.com/kj-9/covid19jp/master/data-raw/dist/pref_daily.csv'
;;

